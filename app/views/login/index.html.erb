<div class="row">
  <div class="panel-header">
    <h2>Log In</h2>
  </div>
  <div class="panel-body">
    <div class="text-left">
      <h3><a class="btn btn-social-icon btn-facebook">
        <span class="fa fa-facebook"></span></a>  &nbsp;
        <%= link_to "Log in with Facebook", user_facebook_omniauth_authorize_path, class: "social-media-link" %></h3>
    </div>
    <div class="text-left">
      <h3><a class="btn btn-social-icon btn-google">
        <span class="fa fa-google"></span></a> &nbsp;
        <%= link_to "Log in with Google", user_google_oauth2_omniauth_authorize_path, class: "social-media-link" %></h3>
    </div>
  </div>
</div>